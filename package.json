{
  "name": "arucas",
  "displayName": "Arucas Language Extension",
  "description": "Language Support for Arucas",
  "version": "2.0.7",
  "publisher": "Arucas",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "arucas",
        "aliases": [
          "Arucas",
          "arucas"
        ],
        "extensions": [
          ".arucas"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "arucas",
        "scopeName": "source.arucas",
        "path": "./syntaxes/arucas.tmLanguage.json"
      }
    ],
    "semanticTokenScopes": [
      {
        "scopes": {
          "storage": [
            "storage.type"
          ]
        }
      }
    ],
    "configuration": {
      "title": "Arucas",
      "properties": {
        "arucas.natives": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "https://raw.githubusercontent.com/senseiwells/Arucas/main/generated/json/AllDocs.json"
          ],
          "description": "Adds autocompletions for the native classes and functions, this provides a list of documentation JSONs."
        }
      }
    }
  },
  "__metadata": {
    "id": "b7d582e4-0efd-439d-a03d-b91e83692d5e",
    "publisherDisplayName": "Arucas",
    "publisherId": "a4c8206b-027a-4fad-af30-006b8edbc993",
    "isPreReleaseVersion": false
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "lint-fix": "eslint src --ext ts --fix",
    "format": "prettier --write .",
    "pretest": "npm run compile && npm run lint && npm run format",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}
